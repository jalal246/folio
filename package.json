{
  "name": "folio",
  "version": "1.0.0",
  "description": "A Form For Everything",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha test/**/*.{jsx,js}",
    "test:comp:w": "cross-env NODE_ENV=test mocha --watch  --watch-extensions jsx test/components/**/*.jsx",
    "test:utils:w": "cross-env NODE_ENV=test mocha --watch  --watch-extensions js test/utils.js",
    "test:karma": "karma start",
    "precover": "rimraf coverage",
    "cover": "cross-env NODE_ENV=test nyc npm run test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "gh-pages:clean": "rimraf _gh-pages",
    "gh-pages:build": "build-storybook -c .storybook -o  _gh-pages",
    "gh-pages:publish": "gh-pages -d  _gh-pages",
    "gh-pages": "npm run gh-pages:clean && npm run gh-pages:build && npm run gh-pages:publish",
    "postpublish": "npm run gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jalal246/folio.git"
  },
  "author": "Jalal Maskoun jimmy002020@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jalal246/folio/issues"
  },
  "homepage": "https://github.com/jalal246/folio#readme",
  "keywords": [
    "react",
    "css-flexbox",
    "css-grid",
    "css-layout",
    "form-builder-api",
    "dynamic-forms"
  ],
  "peerDependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "prop-types": "^15.6.2"
  },
  "dependencies": {},
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.11",
    "@storybook/addon-info": "^3.4.11",
    "@storybook/addon-knobs": "^3.4.11",
    "@storybook/addon-links": "^3.4.11",
    "@storybook/addon-options": "^3.4.11",
    "@storybook/addons": "^3.4.11",
    "@storybook/react": "^3.4.11",
    "babel-core": "^6.26.3",
    "babel-plugin-istanbul": "^5.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^2.0.0",
    "jsdom": "^12.1.0",
    "jsdom-global": "3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "prettier": "^1.14.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "rimraf": "^2.6.2",
